<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>fare_greater_than_5000_then_food</fullName>
    <active>true</active>
    <description>if fare is greater than 5000 then foodpreference is necessary</description>
    <errorConditionFormula>Fare__c  &gt; 5000  &amp;&amp;  ISBLANK( Food_Preferences__c )</errorConditionFormula>
    <errorDisplayField>Food_Preferences__c</errorDisplayField>
    <errorMessage>Please Select Food</errorMessage>
</ValidationRule>
